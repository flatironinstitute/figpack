{
  "name": "figpack_3d",
  "version": "1.0.0",
  "description": "3D visualization extension for figpack using Three.js",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "npm run build && concurrently \"npm run dev:watch\" \"npm run dev:serve\"",
    "dev:watch": "vite build --watch",
    "dev:serve": "vite preview --port 5174"
  },
  "dependencies": {
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/three": "^0.160.0",
    "concurrently": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "terser": "^5.0.0"
  },
  "keywords": [
    "figpack",
    "3d",
    "visualization",
    "three.js"
  ],
  "author": "figpack contributors",
  "license": "Apache-2.0"
}
