{
  "name": "figpack_jfm",
  "version": "1.0.0",
  "description": "A figpack extension package",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "npm run build && concurrently \"npm run dev:watch\" \"npm run dev:serve\"",
    "dev:watch": "vite build --watch",
    "dev:serve": "vite preview --port 5174"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "keywords": [
    "figpack",
    "visualization",
    "neuroscience"
  ],
  "author": "figpack contributors",
  "license": "Apache-2.0"
}
