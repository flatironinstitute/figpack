{
  "name": "@figpack/main-plugin",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cpy \"**/*.css\" \"../dist\" --cwd=src",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "dependencies": {
  },
  "devDependencies": {
    "cpy-cli": "^6.0.0",
    "typescript": "~5.8.3"
  }
}
